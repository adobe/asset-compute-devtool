{
    "name": "client",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@react/collection-view": "file:libraries/react-collection-view-4.1.5.tgz",
        "@react/react-spectrum": "file:libraries/react-react-spectrum-2.25.0.tgz",
        "ace-builds": "^1.4.12",
        "ajv": "^6.12.0",
        "babel-polyfill": "^6.26.0",
        "cross-os": "^1.4.0",
        "debug": "^4.3.1",
        "form-data": "^3.0.0",
        "js-yaml": "^3.13.1",
        "react": "^16.13.0",
        "react-ace": "^9.2.1",
        "react-dom": "^16.13.0",
        "react-scripts": "3.4.0",
        "regenerator-runtime": "^0.13.7",
        "url": "^0.11.0"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "cross-os cross-os-build",
        "eject": "react-scripts eject",
        "test": "react-scripts test",
        "posttest": "license-checker --summary",
        "unit-test": "npm test -- --coverage --watchAll=false"
    },
    "cross-os": {
        "cross-os-build": {
            "darwin": "yarn install && NODE_OPTIONS='--openssl-legacy-provider' react-scripts build && rm -rf ../server/client-build/ && mv build ../server/client-build/",
            "linux": "yarn install && NODE_OPTIONS='--openssl-legacy-provider' react-scripts build && rm -rf ../server/client-build/ && mv build ../server/client-build/",
            "win32": "yarn install && set NODE_OPTIONS=--openssl-legacy-provider && react-scripts build && build.cmd"
        }
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "codecov": "^3.8.1",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.5",
        "jest-fetch-mock": "^3.0.3",
        "license-checker": "^25.0.1",
        "nyc": "^15.1.0"
    },
    "resolutions": {
        "@react/collection-view": "file:libraries/react-collection-view-4.1.5.tgz",
        "minimist": "1.2.8",
        "json5": "1.0.2",
        "decode-uri-component": "0.2.2",
        "terser": "4.8.1",
        "moment": "2.29.4",
        "eventsource": "1.1.1",
        "async": "2.6.4",
        "url-parse": "1.5.10",
        "follow-redirects": "1.14.8"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/components/**/*.{js,jsx}",
            "src/App.js"
        ],
        "coverageReporters": [
            "text",
            "lcov"
        ],
        "moduleNameMapper": {
            "ace-builds": "<rootDir>/node_modules/ace-builds",
            "^node:stream$": "stream-browserify",
            "^node:util$": "util",
            "^node:crypto$": "crypto-browserify",
            "^node:fs$": "fs",
            "^node:path$": "path-browserify",
            "^node:os$": "os-browserify",
            "^node:events$": "events"
        }
    },
    "proxy": "http://localhost:9000"
}
